		${EGO_SUM_SRC_URI}"

LICENSE="BSD-3"
IUSE="pie"
SLOT="0"
KEYWORDS="~amd64 ~x86"

DEPEND="sys-libs/glibc"
RDEPEND="${DEPEND}"
BDEPEND=""

S="${WORKDIR}/${P}"

src_prepare() {
	default

	rm -r .github
	rm LICENSE
}

src_compile() {
	if use pie ; then
		go build \
		-buildmode=pie \
		-trimpath \
		-mod=readonly \
		-modcacherw \
		-ldflags "-X main.version=${PV} -s -w -linkmode=external"  \
		-o "build/${PN}" \
		client/main.go
	else
		go build \
		-trimpath \
		-mod=readonly \
		-modcacherw \
		-ldflags "-X main.version=${PV} -s -w -linkmode=external"  \
		-o "build/${PN}" \
		client/main.go
	fi
}

src_install() {

exeinto /usr/bin
doexe build/wiretrustee

newinitd ${PN}.openrc ${PN}
systemd_dounit "${FILES}/${PN}@.service"

}
